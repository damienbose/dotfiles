// Place your key bindings in this file to override the defaultsauto[]
[
    // {
    //     "key": "cmd+s",  // You can choose your own key combination
    //     "command": "workbench.action.tasks.test",
    //     "when": "editorLangId == cpp"
    // },
    {
        "key": "cmd+1",
        "command": "workbench.action.openEditorAtIndex1",
        "when": ""
    },
    {
        "key": "cmd+2",
        "command": "workbench.action.openEditorAtIndex2",
        "when": ""
    },
    {
        "key": "cmd+3",
        "command": "workbench.action.openEditorAtIndex3",
        "when": ""
    },
    {
        "key": "cmd+4",
        "command": "workbench.action.openEditorAtIndex4",
        "when": ""
    },
    {
        "key": "cmd+5",
        "command": "workbench.action.openEditorAtIndex5",
        "when": ""
    },
    {
        "key": "ctrl+cmd+h",
        "command": "workbench.action.previousEditor",
        "when": ""
    },
    {
        "key": "ctrl+cmd+l",
        "command": "workbench.action.nextEditor",
        "when": ""
    },
    {
        "key": "ctrl+f",
        "command": "workbench.action.toggleFullScreen",
        "when": "!isIOS"
    },
    {
        "key": "ctrl+cmd+f",
        "command": "-workbench.action.toggleFullScreen",
        "when": "!isIOS"
    },
    {
        "key": "ctrl+left",
        "command": "workbench.action.navigateBack",
        "when": "canNavigateBack"
    },
    {
        "key": "ctrl+-",
        "command": "-workbench.action.navigateBack",
        "when": "canNavigateBack"
    },
    {
        "key": "ctrl+right",
        "command": "workbench.action.navigateForward",
        "when": "canNavigateForward"
    },
    {
        "key": "ctrl+shift+-",
        "command": "-workbench.action.navigateForward",
        "when": "canNavigateForward"
    },
    {
        "key": "alt+v",
        "command": "multiclip.list"
    },
    {
        "key": "cmd+h",
        "command": "git.openFile"
    },
    {
        "key": "alt+cmd+v",
        "command": "markdown.showPreviewToSide",
        "when": "!notebookEditorFocused && editorLangId == 'markdown'"
    },
    {
        "key": "cmd+k v",
        "command": "-markdown.showPreviewToSide",
        "when": "!notebookEditorFocused && editorLangId == 'markdown'"
    },
    {
        "key": "shift+cmd+g",
        "command": "workbench.view.scm",
        "when": "workbench.scm.active"
    },
    {
        "key": "ctrl+shift+g",
        "command": "-workbench.view.scm",
        "when": "workbench.scm.active"
    },
    {
        "key": "shift+cmd+b",
        "command": "latex-workshop.recipes"
    },
    {
        "key": "cmd+r cmd+w",
        "command": "editor.debug.action.selectionToWatch"
    },
    {
        "key": "cmd+down",
        "command": "selectNextSuggestion",
        "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus || suggestWidgetVisible && textInputFocus && !suggestWidgetHasFocusedSuggestion"
    },
    {
        "key": "cmd+down",
        "command": "-selectNextSuggestion",
        "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus || suggestWidgetVisible && textInputFocus && !suggestWidgetHasFocusedSuggestion"
    },
    {
        "key": "ctrl+h",
        "command": "workbench.action.navigateLeft"
    },
    {
        "key": "ctrl+l",
        "command": "workbench.action.navigateRight"
    },
    {
        "key": "ctrl+k",
        "command": "workbench.action.navigateUp"
    },
    {
        "key": "ctrl+j",
        "command": "workbench.action.navigateDown"
    },
    {
        "key": "alt+j",
        "command": "editor.action.moveLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+down",
        "command": "-editor.action.moveLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+k",
        "command": "editor.action.moveLinesUpAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+up",
        "command": "-editor.action.moveLinesUpAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+k",
        "command": "-editor.action.accessibilityHelpConfigureKeybindings",
        "when": "accessibilityHelpIsShown && accessibleViewHasUnassignedKeybindings"
    },
    {
        "key": "ctrl+cmd+r",
        "command": "workbench.action.openRecent"
    },
    {
        "key": "ctrl+r",
        "command": "-workbench.action.openRecent"
    },
    {
        "key": "shift+cmd+h",
        "command": "vscode-harpoon.editorQuickPick"
    },
]